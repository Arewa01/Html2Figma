{
    "version": 2,
    "functions": {
        "server.js": {
            "runtime": "@vercel/node@3",
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
