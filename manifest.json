{
    "name": "HTML to Figma Converter",
    "id": "html-to-figma-converter",
    "api": "1.0.0",
    "main": "code.js",
    "ui": "ui.html",
    "editorType": [
        "figma"
    ],
    "networkAccess": {
        "allowedDomains": [
            "https://example.com"
        ],
        "devAllowedDomains": [
            "http://localhost:4000"
        ],
        "reasoning": "The plugin connects to a local backend running on port 4000 for development, and example.com for demo/testing."
    },
    "permissions": [
        "currentuser"
    ],
    "enableProposedApi": false
}
